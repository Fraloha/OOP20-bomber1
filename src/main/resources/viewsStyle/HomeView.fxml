                  <Image url="@../images/PLAY_unset.png" />
               </image>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </ImageView>
            <ImageView fx:id="buttonRank" fitHeight="59.0" fitWidth="148.0" onMouseClicked="#switchToRank" onMouseEntered="#setRank" onMouseExited="#unsetRank" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/RANK_unset.png" />
               </image>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </ImageView>
            <ImageView fx:id="buttonRules" fitHeight="59.0" fitWidth="148.0" onMouseEntered="#setRules" onMouseExited="#unsetRules" onMousePressed="#switchToRules" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/RULES_unset.png" />
               </image>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </ImageView>
         </children>
      </VBox>
   </children>
</AnchorPane>
